{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/test-db.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/test-tables.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/test-db/(.*)",
      "dest": "/api/test-db.js"
    },
    {
      "src": "/test-tables/(.*)",
      "dest": "/api/test-tables.js"
    }
  ],
  "functions": {
    "api/index.js": {
      "maxDuration": 60
    },
    "api/test-db.js": {
      "maxDuration": 30
    },
    "api/test-tables.js": {
      "maxDuration": 30
    }
  },
  "env": {
    "NODE_ENV": "production"
  }
}